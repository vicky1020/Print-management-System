                    <td class="paperSize">@item.PaperSize</td>
                    <td class="paperQuality">@item.PaperQuality</td>
                    <td class="paperSide">@item.PaperSides</td>
                    <td class="ledgerFalio">@item.LedgerFalio</td>
                    <td class="qty">@item.Quantity</td>
                    <td class="amt">@item.Amount</td>
                    <td class="createdBy">@item.CreatedBy</td>
                    <td class="createdDate">@item.CreatedDate</td>
