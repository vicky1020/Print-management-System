        <table id="example" class="display table table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Order Id</th>
                    <th>Product Item</th>
                    <th>Paper GSM</th>
                    <th>Paper Colour</th>
                    <th>Paper Size</th>
                    <th>Paper Quality</th>
                    <th>PaperSides</th>
                    <th>Ledger Falio</th>
                    <th>Qty</th>
                    <th>Amount</th>
                    <th>Created By</th>
                    <th>Created Date</th>
                    <th>Job Status</th>
                </tr>
            </thead>
            @foreach (var item in ViewBag.OrderItem)
            {
                <tr>
                    <td>@item.OrderId</td>
                    <td>@item.ProductItem</td>
                    <td>@item.PaperGSM</td>
                    <td>@item.PaperColour</td>
                    <td>@item.PaperSize</td>
                    <td>@item.PaperQuality</td>
                    <td>@item.PaperSides</td>
                    <td>@item.LedgerFalio</td>
                    <td>@item.Quantity</td>
                    <td>@item.Amount</td>
                    <td>@item.CreatedBy</td>
                    <td>@item.CreatedDate</td>
                    <td>@item.JobProcessType</td>
                </tr>
            }
        </table>
    </div>
</div>
