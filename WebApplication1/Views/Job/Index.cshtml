@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#example').dataTable();

            var elements = $(".job-status");

            for (var i = 0; i < elements.length; i++) {
                element = $(elements[i]);
                element.val(element.attr("value"));
            }


            var customer_type = $("input[name$='customer']:checked").val();
            if (customer_type == "1") {
                $('.client_new').show();
                $('.client_old').hide();
            } else {
                $('.client_old').show();
                $('.client_new').hide();
            }
            $("input[name$='customer']").click(function () {

                if (this.value == "1") { //check value if it is domicilio
                    $('.client_new').show();
                    $('.client_old').hide();
                } else {
                    $('.client_old').show();
                    $('.client_new').hide();
                }
            });

            $("#job_process_type").change(function () {

                var job_type = $('#job_process_type option:selected').val();
                if (job_type == 1) {
                    $('.order_confor_div').removeClass('hide');
                } else {
                    $('.order_confor_div').addClass('hide');
                }
            });


            $("#ADDbtn").click(function () {
                $('.bill_table').removeClass('hide');
                var description = $('#item_card_type').val();
                var qty = $('#bill_count').val()
                var amount = $('#Amount1').val();
                var GST = (amount * 16) / 100;
                var netAmount = parseInt(amount) + GST;
                
                $('#price').html(amount);
                $('#price1').html(amount);
                $('#price2').html(amount);
                $('#GST').html(GST);
                $('#netamount').html(netAmount);
                $('#qty').html(qty);
                $('#description').html(description);
                insertOrder();
              });

            function insertOrder() {
                var customerType;
                var customer_type = $("input[name$='customer']:checked").val();
                if (customer_type == "2") {
                    customerType = $('#customer option:selected').attr("value");

                }
                else {

                    customerType =  @ViewBag.Customers.Count +1;

                }
                var dataObject = {
                    'ProductItem': $('#item_card_type option:selected').val(),
                    'PaperGSM': $('#jc_paper_gsm option:selected').val(),
                    'PaperColour': $('#PaperColor option:selected').val(),
                    'PaperSize': $('#paper_size1 option:selected').val(),
                    'PaperQuality': $('#PaperQuality option:selected').val(),
                    'PaperSides': $('#PaperSide option:selected').val(),
                    'LedgerFalio': $('#Ledget_Falio option:selected').val(),
                    'AdditionalPaperCount': $('#Add_paper_count').val(),
                    'AdditionalPaperSize': $('#paper_size2 option:selected').val(),
                    'AdditonalPaperGSM': $('#paper_gsm2 option:selected').val(),
                    'Quantity': $('#Quantity').val(),
                    'Amount': $('#Amount1').val(),
                    'CreatedBy': 'admin',
                    'CustomerId': customerType,
                    'JobProcessType': $('#job_process_type option:selected').val(),
                    'UpdatedBy': 'admin'

                };
                //if ($("input[name$='customer']:checked").val() == '1') {
                //    var dataObject1 = {   
                //        'FirstName': 'vivek',
                //        'LastName': 'kumar',
                //        'Email': 'vivek@gmail.com',
                //        'PhoneNumber': '9176855954',
                //        'CreatedBy': 'admin'
            };
            //    $.ajax({
            //        url: "/Job/customer",
            //        type: "POST",
            //        data: JSON.stringify({ 'obj': dataObject1 }),
            //        dataType: "json",
            //        traditional: true,
            //        contentType: "application/json; charset=utf-8",
            //        success: function (data) {
            //            if (data.status == "Success") {
            //                alert("Done");
            //            } else {
            //                alert("Error occurs on the Database level!");
            //            }
            //        },
            //        error: function (error) {
            //            alert(JSON.stringify(error));
            //        }
            //    });
            //}
            $.ajax({
                url: "/Job/SaveOrder",
                type: "POST",
                data: JSON.stringify({ 'obj': dataObject }),
                dataType: "json",
                traditional: true,
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    if (data.status == "Success") {
                        alert("Done");
                    } else {
                        alert("Error occurs on the Database level!");
                    }
                },
                error: function (error) {
                    alert(JSON.stringify(error));
                }
            });



        });
    </script>}
